<header>
  <h1>Blogposts#index</h1>
  <p>投稿記事の一覧</p>
</header>
<div class="container">

  <table>
    <tr>
      <th>Id</th><th>Title</th><th>read</th><th>genre</th>
      <th colspan="2"></th>
    </tr>
    <% @data.each do |obj| %>
    <tr>
      <td><%= obj.id %></td>
      <td><%= obj.title %></td>
      <td><%= obj.read %></td>
      <td><%= obj.bloggenre.name %></td>
      <td><a href="/blogposts/<%= obj.id %>" class="editdelete">Edit</a></td>
      <td><a href="/blogposts/delete/<%= obj.id %>" class="editdelete">Delete</a></td>
    </tr>
    <% end %>
  </table>
  <p><a href="/blogposts/add">新規作成</a></p>
</div>
